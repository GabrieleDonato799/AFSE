{
  "name": "gestioneutenti",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon swagger.js -i 'swagger-output.json' & cd .. ; nodemon back/index.js",
    "inspect": "nodemon swagger.js -i 'swagger-output.json' & cd .. ; nodemon --inspect back/index.js",
    "inspect-brk": "nodemon swagger.js -i 'swagger-output.json' & cd .. ; nodemon --inspect-brk back/index.js",
    "bsync": "cd ../front ; browser-sync start -p localhost:3005 -w --files '*.html' --files 'js/*.js' --files 'shared/lib.js' --files 'js/navbar.js' --files 'css/*.css'",
    "jsdoc": "cd .. ; find . -name '*.js' -not -path '*node_modules*' -not -path '*docs*'| paste -s -d' ' | xargs ./back/node_modules/.bin/jsdoc -r -d docs"
  },
  "author": "Gabriele Donato",
  "license": "ISC",
  "description": "Gestione degli utenti e dei loro album",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "isomorphic-fetch": "^3.0.0",
    "mongodb": "^6.8.0",
    "nodemon": "^3.1.3",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "jsdoc": "^4.0.4"
  }
}
